<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin
      name="%pluginName"
      id="org.nasdanika.models.html"
      version="1.0.0"
      provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
      <import plugin="org.nasdanika.exec" export="true"/>
      <import plugin="org.nasdanika.ncore" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated html -->
      <package
            uri="ecore://nasdanika.org/html"
            class="org.nasdanika.html.model.html.HtmlPackage"
            genModel="model/html.genmodel"/>
   </extension>

</plugin>
